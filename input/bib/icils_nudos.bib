@article{punterGenderDifferencesComputer2017,
  title = {Gender Differences in Computer and Information Literacy: {{An}} Exploration of the Performances of Girls and Boys in {{ICILS}} 2013},
  shorttitle = {Gender Differences in Computer and Information Literacy},
  author = {Punter, R Annemiek and Meelissen, Martina RM and Glas, Cees AW},
  year = {2017},
  month = nov,
  journal = {European Educational Research Journal},
  volume = {16},
  number = {6},
  pages = {762--780},
  issn = {1474-9041},
  doi = {10.1177/1474904116672468},
  urldate = {2023-09-20},
  abstract = {IEA's International Computer and Information Literacy Study (ICILS) 2013 showed that in the majority of the participating countries, 14-year-old girls outperformed boys in computer and information literacy (CIL): results that seem to contrast with the common view of boys having better computer skills. This study used the ICILS data to explore whether the achievement test used in this study addressed specific dimensions of CIL and, if so, whether the performances of girls and boys on these subscales differ. We investigated the hypothesis that gender differences in performance on computer literacy items would be slightly in favour of boys, whereas gender differences in performance on information literacy items would be slightly in favour of girls. Furthermore, it was examined whether such differences varied across European countries and if item bias was present. Data was analysed using a confirmative factor analysis model, i.e. a multidimensional item response theory model, for the identification of the subscales, the explorations of gender and national differences, and possible item bias. To a large extent the results support our postulated hypothesis and shed new light on the commonly assumed disadvantaged position of girls and women in modern information society.},
  langid = {english},
  annotation = {El art{\'i}culo parte revisando la literatura donde da cuenta de como las transformaciones que ha sufrido la sociedad, en t{\'e}rminos de digitalizaci{\'o}n de la vida cotidiana, ha comenzado a favorecer el cierre de la brecha digital por g{\'e}nero. En otras palabras, los hallazgos recientes a la fecha van mostrando que una vez masificadas las TIC, las diferencias que favorecer a hombres sobre mujeres en la manipulaci{\'o}n de computadores ha ido disminuyendo. En cambio, aparecen nuevas diferencias que guardan mayor relaci{\'o}n a la diferencias de intereses, que sin embargo, no significan un porblema de desigualdad mayor.\\
\\
Sin embargo, ninguno de estos estudios se pregunta por los sesgos de medici{\'o}n del concepto de ``competencias computacionales'' seg{\'u}n la cultura en cuesti{\'o}n. Por eso, este estudio ocupa ICILS 2013 para verificar la condici{\'o}n universal de las diferencias de g{\'e}nero internacionalmente.\\
\\
En este caso, se ocupan los items de ICILS de otra manera a como lo propone IEA: Se arma una representaci{\'o}n tridimensional de CIL ajustada con un modelo MIRT (Despu{\'e}s de descartar el unidimensional GPCM y el PCM), correlaci{\'o}n estructural y carga de {\'i}tems. Estas tres dimensiones se ven testeadas para ver las diferencias por g{\'e}nero.\\
\\
Las tres dimensiones de CIL son\\
\\
1. Aplicar funcionalidad t{\'e}cnica (Know how to get something done using the technology).\\
    \\
2. Evaluar y reflexionar sobre la informaci{\'o}n (Evaluate information, reflect on using and sharing informativon).\\
    \\
3. Preparar un producto de informaci{\'o}n (Prepare an information output product / share information)\\
    \\
\\
El estudio ocupa solo pa{\'i}ses de Europa que est{\'a}n en ICILS.\\
\\
Los resultados de la prueba MIRT muestran que en realidad la dimensi{\'o}n 2 y 3 miden cuestiones parecidas, por lo que no discriminan como si lo hace la dimensi{\'o}n 1.\\
\\
Los resultados muestran que las mujeres tienen una ventaja sobre los hombres en la dimensiones orientadas a la informaci{\'o}n (2 y 3). En la dimensi{\'o}n 1 no se hallaron diferencias significativas entre g{\'e}neros. Esto fue relativamente consistente en todos los pa{\'i}ses europeos de la medici{\'o}n.}
}

@article{qaziGenderDifferencesInformation2022,
  title = {Gender Differences in Information and Communication Technology Use \& Skills: A Systematic Review and Meta-Analysis},
  shorttitle = {Gender Differences in Information and Communication Technology Use \& Skills},
  author = {Qazi, Atika and Hasan, Najmul and {Abayomi-Alli}, Olusola and Hardaker, Glenn and Scherer, Ronny and Sarker, Yeahia and Kumar Paul, Sanjoy and Maitama, Jaafar Zubairu},
  year = {2022},
  journal = {Education and Information Technologies},
  volume = {27},
  number = {3},
  pages = {4225--4258},
  issn = {1360-2357},
  doi = {10.1007/s10639-021-10775-x},
  urldate = {2024-04-04},
  abstract = {Even though information and communication technology (ICT) is essential for everyday life and has gained considerable attention in education and other sectors, it also carries individual differences in its use and relevant skills. This systematic review aims to examine the gender differences in ICT use and skills for learning through technology. A comprehensive search of eight journal databases and a specific selection criterion was carried out to exclude articles that match our stated exclusion criteria. We included 42 peer-reviewed empirical publications and conference proceedings published between 2006 and 2020. For a subsample of studies, we performed a small-scale meta-analysis to quantify possible gender differences in ICT use and skills. A random-effects model uncovered a small and positive, yet not significant, effect size in favor of boys (g~=\,0.17, 95\% CI [-0.01, 0.36]). However, this finding needs to be further backed by large-scale meta-analyses, including more study samples and a broader set of ICT use and skills measures. We highlight several concerns that should be addressed and more thoroughly in collaboration with one another to better IT skills and inspire new policies to increase the quality of ICT use. The findings from this review further suggest implications and present existing research challenges and point to future research directions.},
  pmcid = {PMC8528947},
  pmid = {34697533}
}

@article{rambaranStabilityChangeStudent2020,
  title = {Stability and Change in Student Classroom Composition and Its Impact on Peer Victimization},
  author = {Rambaran, J. Ashwin and {van Duijn}, Marijtje A. J. and Dijkstra, Jan Kornelis and Veenstra, Ren{\'e}},
  year = {2020},
