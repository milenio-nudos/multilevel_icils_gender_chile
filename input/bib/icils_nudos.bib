@incollection{meelissenComputerAttitudesCompetencies2008,
  title = {Computer {{Attitudes}} and {{Competencies Among Primary}} and {{Secondary Schoolstudents}}},
  booktitle = {International {{Handbook}} of {{Information Technology}} in {{Primary}} and {{Secondary Education}}},
  author = {Meelissen, Martina},
  editor = {Voogt, Joke and Knezek, Gerald},
